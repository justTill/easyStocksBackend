                {
                  "column": {
                    "name": "sectorId",
                    "type": "int",
                    "constraints": {
                      "nullable": false,
                      "unique": true
                    }
                  }
                },
                {
                  "column": {
                    "name": "nameEn",
                    "type": "varchar(255)",
                    "constraints": {
                      "nullable": false
                    }
                  }
                },
                {
                  "column": {
                    "name": "nameDe",
                    "type": "varchar(255)",
                    "constraints": {
                      "nullable": false
                    }
                  }
                }
              ]
            }
          },
          {
            "createTable": {
              "tableName": "stocks",

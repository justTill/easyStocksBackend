                    "constraints": {
                      "nullable": false,
                      "unique": true
                    }
                  }
                },
                {
                  "column": {
                    "name": "sectorId",
                    "type": "int",
                    "constraints": {
                      "nullable": false,
                      "foreignKeyName": "fk_sector_stocks",
                      "references": "sectors(\"sectorId\")"
                    }
                  }
                }
              ]
            }
          },
          {
            "createTable": {
              "tableName": "stocksData",
